  "homepage": "https://www.electronjs.org",
  "scripts": {
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm-run-all -p typecheck:*",
    "prisma:gen": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:clear": "ts-node scripts/clearDb.ts",
    "start:client": "electron-vite preview",
    "start:server": "cross-env SWCRC=true node -r @swc-node/register _server/index.ts",
    "dev:client": "electron-vite dev",
    "dev:server": "nodemon --watch _server --exec cross-env SWCRC=true node --trace-warnings -r @swc-node/register _server/index.ts",
    "dev": "npm-run-all -p dev:*",
    "post:patch": "patch-package && prisma generate",
    "post:install-client-deps": "electron-builder install-app-deps",
    "postinstall": "npm-run-all -p post:*",
    "build:server": "npx swc --config-file ./.swcrc _server -d lib && cp _server/free_mail.json lib/free_mail.json",
    "build:client": "electron-vite build && cp -r out/renderer html",
    "build:win": "npm run build:client && electron-builder --win --config",
    "build:mac": "electron-vite build && electron-builder --mac --config",
    "build:linux": "electron-vite build && electron-builder --linux --config",
    "storybook": "storybook dev -p 6007",
    "build-storybook": "storybook build",
    "get:token": "node scripts/session.js"
  },
