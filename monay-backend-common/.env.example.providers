# Provider API Configuration
# Add these to your .env file once you receive credentials

# Cross River Bank
CROSS_RIVER_API_URL=https://api.crossriver.com/v1
CROSS_RIVER_SANDBOX_URL=https://sandbox.crossriver.com/v1
CROSS_RIVER_API_KEY=your_api_key_here
CROSS_RIVER_API_SECRET=your_api_secret_here
CROSS_RIVER_FBO_ACCOUNT_NUMBER=your_fbo_account_here
CROSS_RIVER_ROUTING_NUMBER=021214891
CROSS_RIVER_WEBHOOK_SECRET=your_webhook_secret_here
CROSS_RIVER_PARTNER_ID=your_partner_id_here

# BitGo Trust
BITGO_API_URL=https://api.bitgo.com/api/v2
BITGO_TEST_API_URL=https://test.bitgo.com/api/v2
BITGO_ACCESS_TOKEN=your_access_token_here
BITGO_ENTERPRISE_ID=your_enterprise_id_here
BITGO_WALLET_PASSPHRASE=your_secure_passphrase_here
BITGO_WEBHOOK_SECRET=your_webhook_secret_here
BITGO_OTP_KEY=your_2fa_key_here

# Tempo (Refactored from blockchain to FX)
TEMPO_API_URL=https://api.tempo.eu/v1
TEMPO_API_KEY=your_api_key_here
TEMPO_API_SECRET=your_api_secret_here
TEMPO_WEBHOOK_SECRET=your_webhook_secret_here
TEMPO_MERCHANT_ID=your_merchant_id_here

# HSM/KMS Configuration (for BitGo)
HSM_ENABLED=false  # Set to true in production
KMS_KEY_ID=your_kms_key_id_here
AWS_REGION=us-east-1